# Always ignore backups of files.
*~

# Ignore some files generated by vim.
\.*\.sw[a-z]
*.un~

# Ignore compiled python files.
*\.py[co]

# Ignore vim tags files.
doc/tags

# Project-specific vimrc.
_vimrc_local.vim
.lvimrc
.vimprj

# Ignore virtualenv files.
.venv
.node-version

# Ignore stupid OSX garbage
.DS_STORE
